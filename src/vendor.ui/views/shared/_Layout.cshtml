<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Marketeq</title>
    <base href="/" />
    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/dist/styles.css" asp-append-version="true" />
    <script>
        System.config({
            packages: {
                'dist': {
                    format: 'cjs',
                    defaultExtension: 'js'
                }
            }
        });
        System.import('dist/main').then(null, console.error.bind(console));
    </script>

</head>
<body>
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
